<Window x:Class="Microsat.Window_Map"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Microsat"
        mc:Ignorable="d"
        Title="Window_Map" Height="2160" Width="3840" ResizeMode="NoResize" Loaded="windowMap_Loaded" WindowStyle="ToolWindow">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser x:Name="webMap" Source="{x:Null}"/>
        <WebBrowser x:Name="webEarth" Grid.Column="1"/>
    </Grid>
</Window>
